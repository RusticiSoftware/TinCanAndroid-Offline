    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher">

        <provider android:name="TCOfflineDataManager"
                  android:authorities="com.rs.TCLocalStorageDatabase"/>

        <activity android:name="TinCanJava_OfflineTests"
                  android:label="@string/app_name">
